{
  "version": "2.0.0",
  "isShellCommand": true,
  "options": {
    "cwd": "${workspaceFolder}"
  },
  "tasks": [
    {
      "label": "build_file",
      "type": "shell",
      "command": "gcc -g -Wall -std=c99 -I${workspaceFolder}/include/ ${workspaceFolder}/src/*.c ${file} -o ./bin/${fileBasenameNoExtension}"
    }
  ]
}
